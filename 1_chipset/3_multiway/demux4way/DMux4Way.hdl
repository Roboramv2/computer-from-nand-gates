/**
 * 4-way demultiplexor:
 * {a, b, c, d} = {in, 0, 0, 0} if sel == 00
 *                {0, in, 0, 0} if sel == 01
 *                {0, 0, in, 0} if sel == 10
 *                {0, 0, 0, in} if sel == 11
 */

CHIP DMux4Way {
    IN in, sel[2];
    OUT a, b, c, d;

    PARTS:
    Nand(a=sel[0], b=sel[0], out=notsel0);
    Nand(a=sel[1], b=sel[1], out=notsel1);
    And(a=notsel0, b=notsel1, out=ca);
    And(a=sel[0], b=notsel1, out=cb); 
    And(a=notsel0, b=sel[1], out=cc);
    And(a=sel[0], b=sel[1], out=cd);
    And(a=in, b=ca, out=a);
    And(a=in, b=cb, out=b);
    And(a=in, b=cc, out=c);
    And(a=in, b=cd, out=d);
}